package entities;

import java.util.Date;

public class Pedido {

	Integer id;
	Date data;
	Double valorTotal;

	
	public Pedido() {
		
		
	}


	public Pedido(Integer id, Date data, Double valorTotal) {

		this.id = id;
		this.data = data;
		this.valorTotal = valorTotal;
	}


	public Integer getId() {
		return id;
	}


	public void setId(Integer id) {
		this.id = id;
	}


	public Date getData() {
		return data;
	}


	public void setData(Date data) {
		this.data = data;
	}


	public Double getValorTotal() {
		return valorTotal;
	}


	public void setValorTotal(Double valorTotal) {
		this.valorTotal = valorTotal;
	}
	
	
	
	
}
